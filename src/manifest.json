{
    "manifest_version": 2,
    "name": "youtube-2-ppt",
    "version": "1.0.1",

    "icons": {
	"48": "images/icon.png"
    },

    "browser_action": {
	"default_icon": {
	    "48": "images/icon.png"
	},
	"default_title": "YouTube2ppt"
    },

    "background": {
	"scripts": [ "player.js", "playerInYT.js", "background.js"]
    },

    "permissions": [
	"<all_urls>",
	"tabs",
	"scripting"
    ]
}
